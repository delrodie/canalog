            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ officine.updatedAt ? officine.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('officine_index') }}">back to list</a>

