{% extends 'base.html.twig' %}

{% block title %}Officine{% endblock %}

{% block body %}
    <h1>Officine</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ officine.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ officine.nom }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ officine.slug }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
